#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;


void main ()
{
	int edad;
	cout <<"Ingrese la edad: ";
	cin>> edad;
	if (edad>=18)
		cout << "puede votar" << endl; 

	else 
		cout << "no puede votar" << endl;
	getch();

    
}

